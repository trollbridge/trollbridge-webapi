<configuration>
  <appSettings>
    <add key="DatabaseConnectionString" value="server=[SERVER_NAME];database=[DB_NAME];uid=[USER_ID];pwd=[PASSWORD];Encrypt=true;TrustServerCertificate=True;Connection Timeout=15"/>
    <add key="RetryCount" value="5"/>
    <add key="RetryInitialDelay" value="00:00:01"/>
    <add key="RetryMinimunDelay" value="00:00:02"/>
    <add key="RetryMaximunDelay" value="00:00:10"/>
    <add key="TransientErrorCodesRetry" value="-2|64|233|08001|08S01|10053|10054|10060|40001|40143|40174|40197|40501|40544|40549|40550|40551|40552|40553|40613|40615"/>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
  </system.web>
  <system.webServer>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers>
  </system.webServer>
